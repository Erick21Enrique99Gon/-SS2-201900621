        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            // TODO: Add your code here
            // bool fireAgain = true;
            try
            {
                string Delimeter = Dts.Variables["User::Delimitador"].Value.ToString();
                string Extension = Dts.Variables["User::ExtensionVentas"].Value.ToString();
                string FolderError = Dts.Variables["User::FolderError"].Value.ToString();
                string FolderOrigen = Dts.Variables["User::FolderOrigen"].Value.ToString();
                string TablaDestino = Dts.Variables["User::TablaPivoteVentas"].Value.ToString();

                string[] fileentries = Directory.GetFiles(FolderOrigen, "SGFood01" + Extension);

                foreach (string fileentry in fileentries)
                {
                    //SqlConnection conn = new SqlConnection();
                    //conn = (SqlConnection)(Dts.Connections[""].AcquireConnection(Dts.Transaction) as SqlConnection);
                    int contador = 0;
                    string linea;
                    System.IO.StreamReader SourceFile = new System.IO.StreamReader(fileentry);
                    while ((linea = SourceFile.ReadLine()) != null)
                    {
                        if (contador > 0)
                        {
                            string[] campos = linea.Split(Delimeter.ToCharArray()[0]);
                            string ruta = "Data Source=localhost; Initial Catalog= TablasPivotes1;Integrated Security = True;";
                            string query = "INSERT INTO " + TablaDestino + " (Fecha, CodigoCliente, NombreCliente, TipoCliente, DireccionCliente, NumeroCliente, CodVendedor, NombreVendedor, Vacacionista, CodProducto, NombreProducto, MarcaProducto, Categoria, SodSuSursal, NombreSucursal, DireccionSucursal, Region, Departamento, Unidades, PrecioUnitario) VALUES (@Fecha, @CodigoCliente, @NombreCliente, @TipoCliente, @DireccionCliente, @NumeroCliente, @CodVendedor, @NombreVendedor, @Vacacionista, @CodProducto, @NombreProducto, @MarcaProducto, @Categoria, @SodSuSursal, @NombreSucursal, @DireccionSucursal, @Region, @Departamento, @Unidades, @PrecioUnitario);";
                            // SqlCommand myCommand = new SqlCommand(query, connection)
                            using (SqlConnection connection = new SqlConnection(ruta))
                            {
                                using (SqlCommand myCommand = new SqlCommand(query, connection))
                                {
                                    myCommand.Parameters.AddWithValue("@Fecha", campos[0]);
                                    myCommand.Parameters.AddWithValue("@CodigoCliente", campos[1]);
                                    myCommand.Parameters.AddWithValue("@NombreCliente", campos[2]);
                                    myCommand.Parameters.AddWithValue("@TipoCliente", campos[3]);
                                    myCommand.Parameters.AddWithValue("@DireccionCliente", campos[4]);
                                    myCommand.Parameters.AddWithValue("@NumeroCliente", campos[5]);
                                    myCommand.Parameters.AddWithValue("@CodVendedor", campos[6]);
                                    myCommand.Parameters.AddWithValue("@NombreVendedor", campos[7]);
                                    myCommand.Parameters.AddWithValue("@Vacacionista", campos[8]);
                                    myCommand.Parameters.AddWithValue("@CodProducto", campos[9]);
                                    myCommand.Parameters.AddWithValue("@NombreProducto", campos[10]);
                                    myCommand.Parameters.AddWithValue("@MarcaProducto", campos[11]);
                                    myCommand.Parameters.AddWithValue("@Categoria", campos[12]);
                                    myCommand.Parameters.AddWithValue("@SodSuSursal", campos[13]);
                                    myCommand.Parameters.AddWithValue("@NombreSucursal", campos[14]);
                                    myCommand.Parameters.AddWithValue("@DireccionSucursal", campos[15]);
                                    myCommand.Parameters.AddWithValue("@Region", campos[16]);
                                    myCommand.Parameters.AddWithValue("@Departamento", campos[17]);
                                    myCommand.Parameters.AddWithValue("@Unidades", campos[18]);
                                    myCommand.Parameters.AddWithValue("@PrecioUnitario", campos[19]);
                                    //System.Windows.Forms.MessageBox.Show($" {myCommand.CommandText} ");
                                    //Dts.Events.FireInformation(0, "Script Task", $"{myCommand.CommandText} ", "", 0,ref fireAgain);
                                    connection.Open();
                                    myCommand.ExecuteNonQuery();
                                    connection.Close();
                                }
                            }

                        }
                        contador++;
                    }
                    SourceFile.Close();
                }
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                using (StreamWriter sw = File.CreateText(Dts.Variables["User::FolderError"].Value.ToString() + "\\" + "ErrorLog.log"))
                {
                    sw.WriteLine("Error: " + ex.Message);
                    sw.WriteLine("Stack Trace: " + ex.StackTrace);
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }
        }

        #region ScriptResults declaration
